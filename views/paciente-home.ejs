<main class="grey lighten-4">
  <div class="row">
    <div class="col m12 valign-wrapper" id="header-registrar">
      <h2 class="white-text accent">Datos Personales del Paciente</h2>
    </div>
  </div>
  <div class="container">
    <form class="row card-panel" method="POST" action="">
      <div class="col m6 s12">
        <div class="row">
          <div class="col s12 input-field">
            <input placeholder="Pedro" id="nombre" name="nombre" type="text" class="validate" value="<%=paciente.Nombre%>">
            <label for="nombre">Nombre Paciente</label>
          </div>
        </div>
        <div class="row">
          <div class="col s12 input-field">
            <input placeholder="Perez" id="apellidoPaciente" type="text" class="validate" value="<%=paciente.Apellido%>">
            <label for="apellidoPaciente">Apellido Paciente</label>
          </div>
        </div>
        <div class="row">
          <div class="col s12 input-field">
            <input placeholder="12345678" id="cedulaPaciente" type="text" class="validate" value="<%=paciente.Cedula%>">
            <label for="cedulaPaciente">Cédula</label>
          </div>
        </div>
        <div class="row">
          <div class="col s12 input-field">
            <input placeholder="AB ORH+" id="tipoSangre" type="text" class="validate" value="<%=paciente.TSangre%>">
            <label for="tipoSangre">Tipo de Sangre</label>
          </div>
        </div>
        <div class="row">
          <div class="col s12 input-field">
            <textarea id="alergiasP" class="materialize-textarea"></textarea>
            <label for="alergiaP">Alergia</label>
          </div>
        </div>
        <div class="row">
          <div class="col s12 input-field">
            <textarea id="antecentesF" class="materialize-textarea"></textarea>
            <label for="antecentesF">Antecedentes Familiares</label>
          </div>
        </div>
      </div>
      <div class="col m6 s12">
        <div class="row">
          <div class="col s12 input-field">
            <input placeholder="Apto. XX Edif. xxxx / Casa N° x" id="namePaciente" type="text" class="validate" value="<%=paciente.NAptoCasa%>">
            <label for="namePaciente">N° de Apto / Casa</label>
          </div>
        </div>
        <div class="row">
          <div class="col s12 input-field">
            <input placeholder="Urb. XXXXXX" id="apellidoPaciente" type="text" class="validate" value="<%=paciente.Urbanizacion%>">
            <label for="apellidoPaciente">Urbanización</label>
          </div>
        </div>
        <div class="row">
          <div class="col s12 input-field">
            <input placeholder="Calle xx / Av xx" id="cedulaPaciente" type="text" class="validate" value="<%=paciente.Calle%>">
            <label for="cedulaPaciente">Calle</label>
          </div>
        </div>
        <div class="row">
          <div class="col s12 input-field">
            <input placeholder="Municipio Sucre / Caracas" id="tipoSangre" type="text" class="validate" value="<%=paciente.Ciudad%>">
            <label for="tipoSangre">Municipio / Ciudad</label>
          </div>
        </div>
        <div class="row">
          <div class="col s12">
            <label for="fechaNacimiento">Fecha de Nacimiento</label>
            <input type="text" class="datepicker" id="fechaNacimiento" value="<%=paciente.FNacimiento%>">
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col s12 center">
          <input class="waves-effect waves-light btn blue darken-1" type="submit" value="Cambiar Datos"/>
        </div>
      </div>
    </form>
  </div>
</main>
<footer class="blue darken-1 page-footer">
  <div class="row">
    <div class="col s12">
      <p class="center white-text"> © 2017 <span class="accent">MedicType Team.</span></p>
    </div>
  </div>
</footer>
